<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="PanePage2" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Scene1.SignUp">
   <children>
      <Label fx:id="WelcomeSignUP" layoutX="146.0" layoutY="24.0" text="Welcome to SignUp page">
         <font>
            <Font size="25.0" />
         </font>
      </Label>
      <Label fx:id="labelName" layoutX="69.0" layoutY="121.0" text="User Name:" />
      <Label fx:id="labelEmail" layoutX="69.0" layoutY="151.0" text="User Email ID:" />
      <Label fx:id="labelPass" layoutX="69.0" layoutY="183.0" text="Password:" />
      <TextField fx:id="TextName" layoutX="162.0" layoutY="117.0" promptText="Enter Your Name" />
      <TextField fx:id="TextEmail" layoutX="162.0" layoutY="147.0" promptText="Enter Your Email id" />
      <TextField fx:id="TextPass" layoutX="162.0" layoutY="179.0" prefHeight="25.0" prefWidth="157.0" promptText=" Give an unique password" />
      <Button fx:id="signUpButton" layoutX="440.0" layoutY="248.0" mnemonicParsing="false" onAction="#sIgnUpButtonAction" text="Sign Up" />
      <Button fx:id="backButton" layoutX="14.0" layoutY="11.0" mnemonicParsing="false" onAction="#BackButtonAction" text="&lt;-" />
      <Text fx:id="errorText" layoutX="395.0" layoutY="126.0" strokeType="OUTSIDE" strokeWidth="0.0" />
   </children>
</AnchorPane>
